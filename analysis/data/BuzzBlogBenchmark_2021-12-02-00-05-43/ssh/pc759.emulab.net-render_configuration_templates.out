worker_processes 8;

events {
  worker_connections 512;
}

http {
  default_type application/octet-stream;
  keepalive_timeout 0;
  upstream backend {
    
    server 172.17.0.1:8080;
    
    server 172.17.0.1:8081;
    
    server 172.17.0.1:8082;
    
    server 172.17.0.1:8083;
    
  }
  server {
    listen 80;
    location / {
      proxy_pass http://backend;
    }
  }
}
bind = "0.0.0.0:81"
workers = 1
threads = 16
